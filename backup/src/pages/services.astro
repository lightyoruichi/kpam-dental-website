---
import BaseLayout from '../components/BaseLayout.astro';
import StructuredData from '../components/StructuredData.astro';

export const prerender = true;

---

<BaseLayout 
	title="Revenue Operations Services | 90-Day Growth Intensive | MIMR Growth Lab"
	description="90-day revenue operations intensive. Fix broken funnels, optimize attribution, automate sales processes. $15K flat rate with performance guarantee for B2B startups."
	canonical="https://mimrgrowthlab.com/services"
	keywords="revenue operations, growth services, funnel optimization, sales automation, B2B growth consultant, revenue guarantee, GTM optimization"
	currentPage="services"
	tallyScript={true}
>
	<!-- Optimized Structured Data -->
	<StructuredData 
		type="services" 
		pageData={{
			title: "90-Day Revenue Operations Intensive",
			description: "Complete revenue operations rebuild with performance guarantee. Fix broken funnels, optimize attribution, automate sales processes.",
			url: "https://mimrgrowthlab.com/services",
			price: "15000",
			faqData: [
				{
					question: "What is included in the 90-day growth plan?",
					answer: "Our plan includes a comprehensive audit, system rebuild, tailored solutions for funnel optimization, and team training to ensure long-term success."
				},
				{
					question: "What challenges do you address specifically?",
					answer: "We focus on optimizing revenue systems, improving conversion rates, automating processes, and establishing effective attribution tracking."
				},
				{
					question: "How many clients do you manage concurrently?",
					answer: "We work with a maximum of 3 clients per quarter to ensure dedicated focus and bespoke service tailored to each client's needs."
				},
				{
					question: "How does the performance guarantee work?",
					answer: "Our guarantee ensures that if projected outcomes aren't realized, you won't be charged. It's a testament to our commitment to results."
				}
			]
		}}
	/>

				<!-- HERO SECTION -->
				<section aria-labelledby="hero-heading" class="text-center">
